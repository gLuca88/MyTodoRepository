<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>MyTodoApp</display-name>
	<welcome-file-list>
		<welcome-file>/login.jsp</welcome-file>

	</welcome-file-list>

	<filter>
		<filter-name>filter1</filter-name>
		<filter-class>GestioneLoginLogOut.LoginFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>filter1</filter-name>
		<url-pattern>*</url-pattern>
	</filter-mapping>

	<servlet>
		<description></description>
		<display-name>ServletLogin</display-name>
		<servlet-name>ServletLogin</servlet-name>
		<servlet-class>GestioneLoginLogOut.ServletLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletLogin</servlet-name>
		<url-pattern>/ServletLogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>GestioneLoginLogOut.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ServletInserimento</display-name>
		<servlet-name>ServletInserimento</servlet-name>
		<servlet-class>GestioneInserimento.ServletInserimento</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletInserimento</servlet-name>
		<url-pattern>/ServletInserimento</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ServletVisualizzazione</display-name>
		<servlet-name>ServletVisualizzazione</servlet-name>
		<servlet-class>GestioneVisualizzazione.ServletVisualizzazione</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletVisualizzazione</servlet-name>
		<url-pattern>/ServletVisualizzazione</url-pattern>
	</servlet-mapping>
</web-app>